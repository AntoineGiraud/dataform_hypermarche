config {
  type: "table",
  description: "Dimension des clients",
  columns: {
    id_client: "id du client",
  },
  assertions: {
    uniqueKey: ["id_client"],
    nonNull:["id_client"]
  }
}

select distinct
    id_client,
    client_nom as nom,
    client_segment as segment
from ${ ref('stg_commande') }
